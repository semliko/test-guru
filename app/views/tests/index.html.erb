<p id="notice"><%= notice %></p>

<h1>Tests</h1>

<table>
  <thead>
  </thead>

  <tbody>
    <% @tests.each do |test| %>
      <tr>
        <td><%= test.category.title %></td>
        <td><%= test.title %></td>
        <td>Questions <%= test.questions.count %></td>
        <td><%= button_to 'Start', start_test_path(test)%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

